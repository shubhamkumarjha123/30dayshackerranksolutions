#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char* readline();
char* ltrim(char*);
char* rtrim(char*);

int parse_int(char*);



int main()
{
    int n;
    
    scanf("%d",&n);
   
    if (n%2==1) 
    {
        if (n%2==1) 
        {
        printf("Weird\n");
        }
        
    }
    else if (n>=2 && n<=5) 
    {
      if  (n%2==0)
      {
          printf("Not Weird");
      }
    
    }
   else if (n>=6 && n<=20) 
   {
      if  (n%2==0)
      {
          printf("Weird");
      }
   }
   else if (n>20)
    {
       if  (n%2==0)
      {
          printf("Not Weird");
      }
   }
    
}
